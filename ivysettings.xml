<ivysettings>
	<settings defaultResolver="default" />
	<include url="${ivy.default.settings.dir}/ivysettings-public.xml" />
	<include url="${ivy.default.settings.dir}/ivysettings-shared.xml" />
	<include url="${ivy.default.settings.dir}/ivysettings-local.xml" />
	<include url="${ivy.default.settings.dir}/ivysettings-main-chain.xml" />
	<include url="${ivy.default.settings.dir}/ivysettings-default-chain.xml" />
	<resolvers>
		<ibiblio name="local-m2" m2compatible="true"
			root="file://${user.home}/.m2/repository" changingPattern=".*dev" />
		<filesystem name="local-m2-publish" m2compatible="true">
			<artifact
				pattern="${user.home}/.m2/repository/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]" />
		</filesystem>
		<ibiblio name="conjars" m2compatible="true"
			root="http://conjars.org/repo/" />
		<ibiblio name="clojars" m2compatible="true"
			root="http://clojars.org/repo/" />
		<ibiblio name="apache" m2compatible="true"
			root="https://repository.apache.org/content/repositories/releases/" />
		<ibiblio name="cloudera" m2compatible="true"
			root="https://repository.cloudera.com/artifactory/cloudera-repos/" />
		<chain name="default" returnFirst="true" checkmodified="true">
			<resolver ref="local" />
			<resolver ref="local-m2" />
			<resolver ref="conjars" />
			<!-- <resolver ref="clojars"/> -->
			<resolver ref="cloudera" />
			<resolver ref="apache" />
			<resolver ref="main" />
		</chain>
	</resolvers>
</ivysettings>